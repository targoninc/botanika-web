{
  "name": "botanika",
  "productName": "Botanika",
  "version": "1.0.89",
  "description": "A local LLM client",
  "repository": {
    "type": "git",
    "url": "https://github.com/targoninc/botanika-desktop.git"
  },
  "private": true,
  "scripts": {
    "test": "vitest run",
    "build-dev": "bun build src/ui/index.ts --outdir ./out --sourcemap=linked --watch",
    "start-dev": "bun --watch src/index.ts",
    "start-dev-all": "bun install && bunx prisma generate && bun run build-dev",
    "build-prod": "bun build src/ui/index.ts --outdir ./out --sourcemap=linked --minify",
    "start-prod": "bun src/index.ts",
    "init-db": "bun src/api/database/initDb.ts",
    "start-with-db": "bun run init-db && bun src/index.ts"
  },
